<div id="map"></div>
<script>
    // Initialize and add the map
    function initMap() {
        // The location of Athens
        var athens = { lat: 37.975535, lng: 23.735073 };
        // The map, centered at Athens
        var map = new google.maps.Map(
            document.getElementById('map'), { zoom: 12, center: athens });
        // The marker, positioned at Athens
        var marker = new google.maps.Marker({ position: athens, map: map });

    };
</script>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDDIANXZR7wMkoTZfgwe8bHqrUKbbrkdDg&callback=initMap"
        type="text/javascript"></script>


<style>
    #map {
        width: 100%;
